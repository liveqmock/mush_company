package com.szmw.dao.tbshop.service;

import java.util.List;
import java.util.Map;

import com.szmw.dao.tbshop.TBShopDAO;
import com.szmw.dao.tbshop.TBShopModel;

public class TBShopServiceImpl implements TBShopService {

	private TBShopDAO dao;

	public int deleteTbShop(TBShopModel tbShop) {
		return this.dao.deleteTbShop(tbShop);
	}

	public int insertTbShop(TBShopModel tbShop) {
		return this.dao.insertTbShop(tbShop);
	}

	public int queryCount(Map<String, Object> params) {
		return this.dao.queryCount(params);
	}

	public TBShopModel queryTbShopModel(TBShopModel tbShop) {
		return this.dao.queryTbShopModel(tbShop);
	}

	public List<TBShopModel> queryTbShopModelList(Map<String, Object> params) {
		return this.dao.queryTbShopModelList(params);
	}

	public int updateTbShop(TBShopModel tbShop) {
		return this.dao.updateTbShop(tbShop);
	}

	public TBShopDAO getDao() {
		return dao;
	}

	public void setDao(TBShopDAO dao) {
		this.dao = dao;
	}
}
